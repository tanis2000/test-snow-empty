			"stopOnEntry": true
		},
		{
			"runtimeExecutable": "${execPath}",
			"args": [
				"runPath=${workspaceRoot}/bin/mac64/",
				"runCommand=snow_empty.app/Contents/MacOS/snow_empty",
				"runInTerminal=false"
			],
			"name": "Debug",
			"type": "hxcpp",
			"request": "launch",
			"program": "",
			"stopOnEntry": true
		}
	]
}
