{
	"version": "0.2.0",
	"configurations": [
		{
			"runtimeExecutable": "${execPath}",
			"args": [
				"compilePath=${workspaceRoot}",
				"compileCommand=haxelib run flow build mac --debug -DHXCPP_DEBUGGER",
